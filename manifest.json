{
    "manifest_version": 3,
    "name": "Weather App",
    "version": "1.0",
    "description": "A weather app extension using OpenWeather API.",
    "permissions": ["geolocation", "activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  